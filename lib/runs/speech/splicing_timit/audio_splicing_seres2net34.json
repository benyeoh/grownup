[
    {
        "datasets": {
            "ktf.datasets.process_chain": {
                "input": {
                    "ktf.datasets.join": {
                        "dataset_tuples": [
                            {
                                "ktf.datasets.speech.splicing_timit.from_tfrecord": {
                                    "tfrecord_dir": "/data/timit/tfrecord_cqt/train",
                                    "validation_split": 0,
                                    "shuffle_size": 200000,
                                    "batch_size": 64
                                }
                            },
                            {
                                "ktf.datasets.speech.splicing_timit.from_tfrecord": {
                                    "tfrecord_dir": "/data/timit/tfrecord_cqt/valid",
                                    "validation_split": 0,
                                    "shuffle_size": 20000,
                                    "batch_size": 64
                                }
                            }
                        ]
                    }
                }
            }
        },
        "model": {
            "ktf.models.OneHeadNet": {
                "base_model": {
                    "ktf.models.networks.seres2net34": {
                        "scale": 8
                    }
                },
                "num_outputs": 2
            }
        },
        "loss": {
            "tf.keras.losses.SparseCategoricalCrossentropy": {
                "from_logits": true
            }
        },
        "metrics": {
            "tf.keras.metrics.SparseCategoricalAccuracy": {}
        },
        "optimizer": {
            "tf.keras.optimizers.Adam": {
                "learning_rate": 1e-3,
                "beta_2": 0.98,
                "epsilon": 1e-09,
                "amsgrad": true
            }
        },
        "train_loop": {
            "ktf.train.KerasTrainLoop": {
                "num_epochs": 150,
                "save_dir": null,
                "callbacks": [
                    {
                        "tf.keras.callbacks.ModelCheckpoint": {
                            "filepath": "/home/klass/exp/checkpoint/weights.epoch{epoch:02d}-val_loss{val_loss:.2f}.h5",
                            "save_weights_only": true
                        }
                    },
                    {
                        "tf.keras.callbacks.EarlyStopping": {
                            "patience": 10
                        }
                    },
                    {
                        "ktf.train.callbacks.Evaluate": {
                            "dataset": {
                                "ktf.datasets.speech.splicing_timit.from_tfrecord": {
                                    "tfrecord_dir": "/data/timit/tfrecord_cqt/test",
                                    "validation_split": 0,
                                    "batch_size": 64
                                }
                            },
                            "mode": "auto",
                            "monitor": "val_loss"
                        }
                    }
                ]
            }
        }
    }
]
