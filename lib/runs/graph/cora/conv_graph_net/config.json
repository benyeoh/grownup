[
    {
        "datasets": {
            "ktf.datasets.graph.cora.from_generator_link_predict": {
                "raw_dir": "/hpc-datasets/graph/cora/raw",
                "max_depth": 2,
                "max_neighbours": 20,
                "max_nodes": 200,
                "batch_size": 128,
                "num_parallel_calls": 8
            }
        },
        "model": {
            "ktf.models.Sequential": {
                "layers": [
                    {
                        "ktf.models.networks.ConvGraphNet": {
                            "layers": [
                                2, 2
                            ],
                            "hidden_base_feature_size": 64,
                            "output_feature_size": 64,
                            "use_residuals": true,
                            "dropout": 0.2
                        }
                    },
                    {
                        "ktf.models.components.GraphLinkPredictHead": {
                            "scale": 1.0,
                            "activation": "relu"
                        }
                    }
                ]
            }
        },
        "loss": {
            "tf.keras.losses.BinaryCrossentropy": {
                "from_logits": false
            }
        },
        "metrics": [
            {
                "tf.keras.metrics.BinaryAccuracy": {}
            }
        ],
        "optimizer": {
            "tfa.optimizers.AdamW": {
                "learning_rate": 0.0013,
                "weight_decay": 0.0
            }
        },
        "train_loop": {
            "ktf.train.KerasTrainLoop": {
                "num_epochs": 30,
                "num_steps": 100,
                "num_valid_steps": 30,
                "save_dir": null
        }
    }
}

]
